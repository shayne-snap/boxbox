{
  "name": "boxbox",
  "version": "0.1.0",
  "packageManager": "pnpm@8.15.8",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./config": {
      "types": "./dist/esm/config/index.d.ts",
      "import": "./dist/esm/config/index.js",
      "require": "./dist/cjs/config/index.cjs"
    },
    "./exec": {
      "types": "./dist/esm/exec/index.d.ts",
      "import": "./dist/esm/exec/index.js",
      "require": "./dist/cjs/exec/index.cjs"
    },
    "./network-proxy": {
      "types": "./dist/esm/network-proxy/index.d.ts",
      "import": "./dist/esm/network-proxy/index.js",
      "require": "./dist/cjs/network-proxy/index.cjs"
    },
    "./logging/sandbox-log": {
      "types": "./dist/esm/logging/sandbox-log.d.ts",
      "import": "./dist/esm/logging/sandbox-log.js",
      "require": "./dist/cjs/logging/sandbox-log.cjs"
    },
    "./fs/volumes": {
      "types": "./dist/esm/fs/volumes.d.ts",
      "import": "./dist/esm/fs/volumes.js",
      "require": "./dist/cjs/fs/volumes.cjs"
    },
    "./util/domain-match": {
      "types": "./dist/esm/util/domain-match.d.ts",
      "import": "./dist/esm/util/domain-match.js",
      "require": "./dist/cjs/util/domain-match.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json && node scripts/rename-cjs.js",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/rename-cjs.js",
    "clean": "rm -rf dist",
    "lint": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.cjs.json --noEmit",
    "prepack": "npm run build",
    "prepare": "npm run build",
    "test": "node --test test/**/*.mjs"
  },
  "dependencies": {
    "default-gateway": "^7.2.2",
    "zod": "^3.24.1"
  },
  "peerDependencies": {
    "@boxlite-ai/boxlite": "*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "~5.6.2"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/boxbox"
  },
  "bugs": {
    "url": "https://github.com/your-org/boxbox/issues"
  },
  "homepage": "https://github.com/your-org/boxbox#readme",
  "keywords": [
    "sandbox",
    "boxlite",
    "sdk",
    "security",
    "container"
  ]
}
